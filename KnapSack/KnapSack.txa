WHEN '?Reorder'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
  DeleteAndLoad = 1
  DO DeleteInvoices
  
  CurrentProcess = '2/2 Rewriting Invoices'
  DISPLAY(?CurrentProcess)
  STREAM(INVOICES)
  LOOP CT = 1 TO RECORDS(InvoicesQueue)
    GET(InvoicesQueue,CT)
    ProgressBar = CT
    DISPLAY(?ProgressBar)
    IF ProgressBar % 10 = 0 THEN YIELD() END

    Inv:InvoiceID = CT
    Inv:Amount    = InvoicesQueue.Amount
    ADD(Invoices)
  END
  FLUSH(INVOICES)
  FREE(InvoicesQueue)

  HIDE(?CurrentProcess)
  HIDE(?ProgressBar)
  BRW1.ResetFromView()
  ThisWindow.Reset(1)
