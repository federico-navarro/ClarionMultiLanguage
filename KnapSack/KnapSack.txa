[PROCEDURE]
NAME MarkTime
PROTOTYPE '(<<*Long ParStartDate>,<<*Long ParStartTime>,<<Long DontReset>),Long,Proc'
PARAMETERS '(<<*Long ParStartDate>,<<*Long ParStartTime>,<<Long DontReset>)'
GLOBAL
[COMMON]
DESCRIPTION 'Stopwatch'
FROM ABC Source
MODIFIED '2017/09/06' ' 0:32:59'
[PROMPTS]
%GenerateOpenClose LONG  (0)
%GenerateSaveRestore LONG  (0)
[EMBED]
EMBED %DataSection
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END

StartDate LONG,STATIC
StartTime LONG,STATIC

CurrentDate LONG
CurrentTime LONG
MaxTime     EQUATE(24*60*60*100)

ELLAPSED    Long
[END]
EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  CurrentDate = TODAY()
  CurrentTime = CLOCK()
  
  IF CurrentDate < TODAY()
     CurrentDate = TODAY()
     CurrentTime = CLOCK()
  END

  IF NOT OMITTED(1) THEN ParStartDate = CurrentDate END
  IF NOT OMITTED(2) THEN ParStartTime = CurrentTime END
  
  Ellapsed = (CurrentDate - StartDate) * MaxTime + CurrentTime - StartTime 
    
  IF OMITTED(3) OR DontReset = 0
    StartDate = CurrentDate
    StartTime = CurrentTime
  END
  
  RETURN Ellapsed
[END]
[END]
